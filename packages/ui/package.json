  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "@web3icons/core": "^4.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "vite": "^5.1.6",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6",
    "typescript": "^5.4.2",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.35",
    "autoprefixer": "^10.4.17"
  },
  "keywords": [
    "vue",
    "vue3",
    "web3",
    "ui",
    "components",
    "tailwind",
    "blockchain",
    "ethereum",
    "wallet"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/thenightproject/supaweb3.git",
