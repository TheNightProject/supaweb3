{
  "name": "@thenightproject/supaweb3-ui",
  "version": "0.2.4",
  "description": "Vue 3 Web3 UI components built with Tailwind CSS",
  "homepage": "https://supaweb3.com",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
